# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/extrapl_db

# API Keys (Replace with your actual keys)
GEMINI_API_KEY=your-gemini-api-key-here
GOOGLE_API_KEY=your-google-api-key-here
OPENAI_API_KEY=your-openai-api-key-here

# Session Configuration
SESSION_SECRET=your-session-secret-here-change-in-production

# Server Configuration
PORT=5000
NODE_ENV=development

# Object Storage Configuration (if used)
OBJECT_STORAGE_URL=https://your-storage-url.com
OBJECT_STORAGE_KEY=your-storage-key-here
OBJECT_STORAGE_SECRET=your-storage-secret-here

# Application Settings
APP_NAME=extrapl
MAX_FILE_SIZE=10485760
ALLOWED_FILE_TYPES=pdf,xlsx,xls,docx,doc,csv,txt,png,jpg,jpeg

# Security Settings
JWT_SECRET=your-jwt-secret-here-change-in-production
JWT_EXPIRY=7d

# Email Configuration (if needed)
SMTP_HOST=smtp.example.com
SMTP_PORT=587
SMTP_USER=your-email@example.com
SMTP_PASS=your-email-password

# Feature Flags
ENABLE_DEBUG_LOGGING=false
ENABLE_AI_FEATURES=true
ENABLE_DOCUMENT_PROCESSING=true